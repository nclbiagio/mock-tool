{
   "name": "dev-proxy-server",
   "version": "1.0.0",
   "description": "development server app",
   "main": "dist/index.js",
   "scripts": {
      "start": "ts-node-dev --respawn --pretty --transpile-only index.ts",
      "build": "tsc --project ./"
   },
   "author": "ncl88",
   "license": "MIT",
   "dependencies": {
      "axios": "^0.21.1",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "helmet": "^4.4.1",
      "http-proxy-middleware": "^1.0.6",
      "jsonwebtoken": "^8.5.1",
      "morgan": "^1.10.0",
      "winston": "^3.3.3"
   },
   "devDependencies": {
      "@types/axios": "^0.14.0",
      "@types/cors": "^2.8.12",
      "@types/dotenv": "^8.2.0",
      "@types/express": "^4.17.11",
      "@types/helmet": "^4.0.0",
      "@types/jsonwebtoken": "^8.5.1",
      "@types/morgan": "^1.9.2",
      "@types/node": "^14.14.31",
      "@typescript-eslint/eslint-plugin": "^4.16.1",
      "eslint": "^7.21.0",
      "eslint-config-airbnb-typescript": "^12.3.1",
      "eslint-plugin-import": "^2.22.1",
      "husky": "^7.0.4",
      "lint-staged": "^12.1.4",
      "mrm": "^2.6.0",
      "nodemon": "^2.0.7",
      "prettier": "^2.2.1",
      "ts-node": "^9.1.1",
      "ts-node-dev": "^1.1.6",
      "typescript": "^4.2.3"
   },
   "husky": {
      "hooks": {
         "pre-commit": "lint-staged"
      }
   },
   "lint-staged": {
      "*.{js,ts}": [
         "prettier --write",
         "eslint --cache --fix",
         "eslint *.ts --fix-dry-run"
      ]
   }
}
